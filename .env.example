# Server configuration
PORT=3000
HOST=localhost

# Log level (debug, info, warn, error)
LOG_LEVEL=info

# Node environment (development, production)
NODE_ENV=development

# Server mode (stdio or sse)
SERVER_MODE=sse

# Starknet configuration
STARKNET_RPC_URL=https://starknet-mainnet.blastapi.io/YOUR_API_KEY/rpc/v0_7
STARKNET_ACCOUNT_ADDRESS=YOUR_ACCOUNT_ADDRESS
STARKNET_ACCOUNT_PRIVATE_KEY=YOUR_PRIVATE_KEY